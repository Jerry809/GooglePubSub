{
  "metadata": [
    {
      "src": { },
      "dest": "obj/api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [ "*.yml" ],
        "src": "obj/api",
        "dest": "api"
      },
      {
        "files": [ "index.md", "**/toc.yml", "**/*.md" ],
      }
    ],
    "resource": "**/*.svg",
    "globalMetadata": {
      "_appTitle": "Google Cloud APIs",
      "_disableContribution": true,
      "_appFooter": " "
    },
    "dest": "site"
  }
}