<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <IsPackable>false</IsPackable>
    <Description>Console app to show the reverse dependencies on NuGet packages</Description>
    <TargetFramework>netcoreapp1.0</TargetFramework>
    <AssemblyName>Google.Cloud.Tools.ShowReverseDependencies</AssemblyName>
    <OutputType>Exe</OutputType>
    <PackageId>Google.Cloud.Tools.ShowReverseDependencies</PackageId>
    <RuntimeFrameworkVersion>1.0.4</RuntimeFrameworkVersion>
    <TreatWarningsAsErrors>True</TreatWarningsAsErrors>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="10.0.2" />
  </ItemGroup>

</Project>
