<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <IsPackable>false</IsPackable>
    <Description>Console app to generate project files based on API descriptions</Description>
    <TargetFramework>netcoreapp1.0</TargetFramework>
    <AssemblyName>Google.Cloud.Tools.ProjectGenerator</AssemblyName>
    <OutputType>Exe</OutputType>
    <PackageId>Google.Cloud.Tools.ProjectGenerator</PackageId>
    <RuntimeFrameworkVersion>1.0.4</RuntimeFrameworkVersion>
    <TreatWarningsAsErrors>True</TreatWarningsAsErrors>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\Google.Cloud.Tools.Common\Google.Cloud.Tools.Common.csproj" />
  </ItemGroup>

</Project>
